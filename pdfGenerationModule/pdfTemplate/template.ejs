      <script>
        if (!<%= checklist.arrive.checked %>) {
          var checkboxs = document.getElementsByClassName("arrival");
        }else if (!<%= checklist.depart.checked %>) {
          var checkboxs = document.getElementsByClassName("departure");
        }
        // var checkbox = document.getElementsByClassName("some-checkbox");
        // checkbox.indeterminate = true;

        [].forEach.call(checkboxs, function (checkbox) {
          checkbox.indeterminate = true;
          console.log(checkbox)
        });
      </script>
    -->
  </body>
</html>
